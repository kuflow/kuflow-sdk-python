{
    "name": "@kuflow/kuflow-rest-generator",
    "version": "0.0.1",
    "private": true,
    "devDependencies": {
      "autorest": "3.6.3"
    },
    "scripts": {
      "autorest": "autorest",
      "generate": "npm run generate:venv-activate && npm run generate:autorest && npm run generate:black && npm run generate:workaround",
